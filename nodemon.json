{
  "exec": "node --trace-deprecation --trace-warnings --zero-fill-buffers src/cli.js",
  "restartable": "rs",
  "quiet": "true",
  "ext": "js",
  "events": {
    "restart": "echo \"[NODEMON] Restarting.\""
  },
  "ignore": [
    "node_modules",
    "package*.json",
    "nodemon.json",
    "test"
  ]
}
